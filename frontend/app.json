{
  "expo": {
    "name": "maanikya",
    "slug": "maanikya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plstforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSCameraUsageDescription": "We need access to your camera to take a photo.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to select a photo.",
        "NSPhotoLibraryAddUsageDescription": "We need access to add photos to your library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
